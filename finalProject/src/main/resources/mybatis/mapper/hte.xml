<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codingbamboo.finalproject.hte.dao.IHteDAO">
	<select id="getHte" resultType="com.codingbamboo.finalproject.hte.dto.HteDTO">
