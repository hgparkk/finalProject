<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codingbamboo.finalproject.rae.dao.IRaeDAO">
	<select id="getRae" resultType="com.codingbamboo.finalproject.rae.dto.RaeDTO">
