<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codingbamboo.finalproject.hmc.dao.IHmcDAO">
	<select id="getHmc" resultType="com.codingbamboo.finalproject.hmc.dto.HmcDTO">
		select
			hmc_number
		    , hmc_count
		    , hmc_count_ratio
		FROM
			household_member_count
	</select>
</mapper>
